package ExercicioPolicial;

public class Policial {
	private String cpf;
	private String nome;
	private double salario;
	
	public String getCpf() {
		return cpf;
	}

	public String getNome() {
		return nome;
	}

	public double getSalario() {
		return salario;
	}

	public Policial(String cpf, String nome, double salario) {
		super();
		this.cpf = cpf;
		this.nome = nome;
		this.salario = salario;
	}
	
	
	
}
